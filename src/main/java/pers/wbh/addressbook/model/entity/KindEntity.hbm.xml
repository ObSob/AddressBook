<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="pers.wbh.addressbook.model.entity.KindEntity" table="kind" schema="adressbook">
        <id name="kindId">
            <column name="kindId" sql-type="int(11)"/>
        </id>
        <property name="kindName">
            <column name="kindName" sql-type="varchar(10)" length="10"/>
        </property>
    </class>
</hibernate-mapping>